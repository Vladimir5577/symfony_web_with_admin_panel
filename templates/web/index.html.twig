        const href = a.getAttribute("href");
        if(!href || href === "#") return;
        const el = document.querySelector(href);
        if(!el) return;
        e.preventDefault();
        el.scrollIntoView({behavior:"smooth", block:"start"});
        history.pushState(null, "", href);
